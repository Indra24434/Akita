{
    "name": "codeigniter4/appstarter",
    "require": {
        "php": ">=8.1",
        "codeigniter4/framework": "4.4.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Config\\": "app/Config/"
        }
    },
    "config": {
        "platform-check": false,
        "optimize-autoloader": true
    },
    "scripts": {
        "railway-setup": "php spark key:generate --force"
    }
}